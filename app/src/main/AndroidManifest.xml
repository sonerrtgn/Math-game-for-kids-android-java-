<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tutorial_one">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tutorial_one">
        <activity android:name=".carpma_oyunu"></activity>
        <activity android:name=".bolme_oyunu" />
        <activity android:name=".toplama_oyunu" />
        <activity android:name=".carpma_seviyeleri" />
        <activity android:name=".bolme_seviyeleri" />
        <activity android:name=".Toplama_seviyeleri" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>